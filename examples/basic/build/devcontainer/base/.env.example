# Generated by stackbuilder from concatenated .env.example files
# Source files: base/.env.example, devcontainer/.env.example

# Base application configuration
APP_NAME=stackbuilder-basic
APP_VERSION=1.0.0
APP_PORT=3000

# Database configuration
DB_HOST=localhost
DB_PORT=5432
