# Generated by stackbuilder from multiple .env.example files
# Source files: base/.env.example, devcontainer/.env.example, guard/.env.example

# Base application configuration
APP_NAME=stackbuilder-basic

APP_PORT=3000

APP_VERSION=1.0.0

# Authentication guard
AUTH_REQUIRED=true

AUTH_TIMEOUT=1800

CORS_ALLOWED_ORIGINS=https://localhost:3000

# Development database
DB_HOST=db

DB_NAME=app_db

DB_PASSWORD=dev_password

DB_PORT=5432

DB_USER=dev_user

# Development container environment
DEBUG=true

# Development tools
ENABLE_PROFILER=true

# Guard extension configuration
GUARD_ENABLED=true

GUARD_LOG_LEVEL=warn

HOT_RELOAD=true

LOG_LEVEL=debug

MOCK_EXTERNAL_APIS=true

RATE_LIMIT_BURST=20

# Rate limiting
RATE_LIMIT_ENABLED=true

RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Security settings
SECRET_KEY=your_secret_key_here

# Security headers
SECURITY_HEADERS_ENABLED=true

