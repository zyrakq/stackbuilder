# Generated by stackbuilder from multiple .env.example files
# Source files: base/.env.example, internal/.env.example, guard/.env.example

# Base application configuration
APP_NAME=stackbuilder-basic

APP_PORT=3000

APP_VERSION=1.0.0

# Authentication guard
AUTH_REQUIRED=true

AUTH_TIMEOUT=1800

CORS_ALLOWED_ORIGINS=https://localhost:3000

# Database settings for internal use
DB_HOST=internal-db

DB_NAME=app_db

DB_PASSWORD=internal_secure_password

DB_PORT=5432

DB_USER=internal_user

# Internal environment configuration
DEBUG=false

ENVIRONMENT=internal

EXTERNAL_ACCESS=false

# Guard extension configuration
GUARD_ENABLED=true

GUARD_LOG_LEVEL=warn

HEALTH_CHECK_INTERVAL=30s

# Internal network settings
INTERNAL_NETWORK=true

LOG_LEVEL=info

# Monitoring
METRICS_ENABLED=true

RATE_LIMIT_BURST=20

# Rate limiting
RATE_LIMIT_ENABLED=true

RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Security settings
SECRET_KEY=your_secret_key_here

# Security headers
SECURITY_HEADERS_ENABLED=true

