# Guard extension configuration
GUARD_ENABLED=true
GUARD_LOG_LEVEL=warn

# Rate limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=20

# Security headers
SECURITY_HEADERS_ENABLED=true
CORS_ALLOWED_ORIGINS=https://localhost:3000

# Authentication guard
AUTH_REQUIRED=true
AUTH_TIMEOUT=1800