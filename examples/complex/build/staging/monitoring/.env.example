# Generated by stackbuilder from concatenated .env.example files
# Source files: base/.env.example, monitoring/.env.example

# Complex application base configuration
APP_NAME=stackbuilder-complex
APP_VERSION=2.0.0
APP_ENV=production

# Core database settings
DATABASE_URL=postgresql://user:password@localhost:5432/complex_db
DB_POOL_SIZE=10
DB_TIMEOUT=30

# Redis cache
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Basic logging
LOG_LEVEL=info
LOG_FORMAT=json
# Monitoring extension
MONITORING_ENABLED=true
METRICS_PORT=9090
METRICS_PATH=/metrics

# Override log level for detailed monitoring
LOG_LEVEL=debug
LOG_FORMAT=structured

# Prometheus settings
PROMETHEUS_ENABLED=true
PROMETHEUS_SCRAPE_INTERVAL=15s
PROMETHEUS_RETENTION=15d

# Grafana integration
GRAFANA_ENABLED=true
GRAFANA_ADMIN_PASSWORD=admin_password

# Health checks
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health
HEALTH_CHECK_INTERVAL=10s

# Override cache TTL for monitoring data
CACHE_TTL=300