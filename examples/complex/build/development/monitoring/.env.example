# Generated by stackbuilder from multiple .env.example files
# Source files: base/.env.example, development/.env.example, monitoring/.env.example

# Development environment settings
APP_ENV=development

# Complex application base configuration
APP_NAME=stackbuilder-complex

APP_VERSION=2.0.0

# Override cache TTL for monitoring data
CACHE_TTL=300

# Development database
DATABASE_URL=postgresql://dev:dev@localhost:5432/complex_dev

DB_POOL_SIZE=5

DB_TIMEOUT=30

DEBUG=true

ENABLE_PROFILER=true

ENABLE_QUERY_LOGGING=true

GRAFANA_ADMIN_PASSWORD=admin_password

# Grafana integration
GRAFANA_ENABLED=true

# Health checks
HEALTH_CHECK_ENABLED=true

HEALTH_CHECK_INTERVAL=10s

HEALTH_CHECK_PATH=/health

# Development tools
HOT_RELOAD=true

LOG_FORMAT=structured

# Override log level for detailed monitoring
LOG_LEVEL=debug

METRICS_PATH=/metrics

METRICS_PORT=9090

MOCK_EMAIL_SERVICE=true

# Mock services for development
MOCK_PAYMENT_SERVICE=true

# Monitoring extension
MONITORING_ENABLED=true

# Prometheus settings
PROMETHEUS_ENABLED=true

PROMETHEUS_RETENTION=15d

PROMETHEUS_SCRAPE_INTERVAL=15s

# Redis cache
REDIS_URL=redis://localhost:6379/0

