# Generated by stackbuilder from multiple .env.example files
# Source files: base/.env.example, development/.env.example, auth/.env.example

# Development environment settings
APP_ENV=development

# Complex application base configuration
APP_NAME=stackbuilder-complex

APP_VERSION=2.0.0

# Authentication extension
AUTH_ENABLED=true

AUTH_PROVIDER=oauth2

AUTH_SESSION_TIMEOUT=3600

CACHE_TTL=3600

# Development database
DATABASE_URL=postgresql://dev:dev@localhost:5432/complex_dev

DB_POOL_SIZE=5

DB_TIMEOUT=30

DEBUG=true

EMAIL_VERIFICATION_REQUIRED=true

ENABLE_PROFILER=true

ENABLE_QUERY_LOGGING=true

# Development tools
HOT_RELOAD=true

JWT_EXPIRY=86400

JWT_REFRESH_ENABLED=true

# JWT settings
JWT_SECRET=your_jwt_secret_key

LOG_FORMAT=json

# Basic logging
LOG_LEVEL=debug

MOCK_EMAIL_SERVICE=true

# Mock services for development
MOCK_PAYMENT_SERVICE=true

# OAuth2 configuration
OAUTH2_CLIENT_ID=your_oauth2_client_id

OAUTH2_CLIENT_SECRET=your_oauth2_client_secret

OAUTH2_REDIRECT_URL=https://app.example.com/auth/callback

# Redis cache
REDIS_URL=redis://localhost:6379/0

# User management
USER_REGISTRATION_ENABLED=true

