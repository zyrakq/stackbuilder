# Generated by stackbuilder from concatenated .env.example files
# Source files: base/.env.example, development/.env.example

# Complex application base configuration
APP_NAME=stackbuilder-complex
APP_VERSION=2.0.0
APP_ENV=production

# Core database settings
DATABASE_URL=postgresql://user:password@localhost:5432/complex_db
DB_POOL_SIZE=10
DB_TIMEOUT=30

# Redis cache
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Basic logging
LOG_LEVEL=info
LOG_FORMAT=json
# Development environment settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=debug

# Development database
DATABASE_URL=postgresql://dev:dev@localhost:5432/complex_dev
DB_POOL_SIZE=5

# Development tools
HOT_RELOAD=true
ENABLE_QUERY_LOGGING=true
ENABLE_PROFILER=true

# Mock services for development
MOCK_PAYMENT_SERVICE=true
MOCK_EMAIL_SERVICE=true