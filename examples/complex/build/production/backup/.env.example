# Generated by stackbuilder from multiple .env.example files
# Source files: base/.env.example, production/.env.example

# Production environment settings
APP_ENV=production

# Complex application base configuration
APP_NAME=stackbuilder-complex

APP_VERSION=2.0.0

CACHE_TTL=7200

CSRF_PROTECTION=true

# Production database (override base settings)
DATABASE_URL=postgresql://prod_user:prod_password@prod-db:5432/complex_prod

DB_POOL_SIZE=20

DB_TIMEOUT=10

DEBUG=false

# Performance settings
ENABLE_COMPRESSION=true

LOG_FORMAT=json

# Basic logging
LOG_LEVEL=warn

RATE_LIMITING=true

# Production Redis
REDIS_URL=redis://prod-redis:6379/0

# Security settings
SECURE_HEADERS=true

STATIC_FILES_CDN=https://cdn.example.com

