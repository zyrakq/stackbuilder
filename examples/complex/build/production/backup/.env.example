# Generated by stackbuilder from concatenated .env.example files
# Source files: base/.env.example, production/.env.example

# Complex application base configuration
APP_NAME=stackbuilder-complex
APP_VERSION=2.0.0
APP_ENV=production

# Core database settings
DATABASE_URL=postgresql://user:password@localhost:5432/complex_db
DB_POOL_SIZE=10
DB_TIMEOUT=30

# Redis cache
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Basic logging
LOG_LEVEL=info
LOG_FORMAT=json
# Production environment settings
APP_ENV=production
DEBUG=false
LOG_LEVEL=warn

# Production database (override base settings)
DATABASE_URL=postgresql://prod_user:prod_password@prod-db:5432/complex_prod
DB_POOL_SIZE=20
DB_TIMEOUT=10

# Production Redis
REDIS_URL=redis://prod-redis:6379/0
CACHE_TTL=7200

# Security settings
SECURE_HEADERS=true
CSRF_PROTECTION=true
RATE_LIMITING=true

# Performance settings
ENABLE_COMPRESSION=true
STATIC_FILES_CDN=https://cdn.example.com